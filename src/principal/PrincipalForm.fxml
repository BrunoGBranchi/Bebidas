<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane fx:id="bpPrincipal" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="principal.PrincipalController">
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar prefHeight="26.0" prefWidth="800.0">
               <menus>
                  <Menu mnemonicParsing="false" text="Menu">
                     <items>
                        <MenuItem fx:id="mmiProdutos" mnemonicParsing="false" onAction="#mmiProdutos" text="Cadastrar Produtos" />
                        <MenuItem fx:id="mmiClientes" mnemonicParsing="false" onAction="#mmiClientes" text="Cadastrar Clientes" />
                        <MenuItem fx:id="mmiCategorias" mnemonicParsing="false" onAction="#mmiCategoria" text="Cadastrar Categorias" />
                        <MenuItem fx:id="mmiUsuarios" mnemonicParsing="false" onAction="#mmiUsuarios" text="Cadastrar Usuarios" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Sobre">
                     <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Sair">
                     <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
         </children>
      </Pane>
   </center>
</BorderPane>
